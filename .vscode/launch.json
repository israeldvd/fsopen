{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "skipFiles": ["<node_internals>/**"],
      "program": "${file}"
    },
    {
      "type": "node-terminal",
      "name": "Run Script: start",
      "request": "launch",
      "command": "npm run start",
      "cwd": "${workspaceFolder}\\part2\\phonebook"
    },
    {
      "command": "npm run start",
      "name": "Run phonebook backend",
      "request": "launch",
      "type": "node-terminal",
      "cwd": "${workspaceFolder}\\part3\\phonebook-backend"
    }
  ]
}
